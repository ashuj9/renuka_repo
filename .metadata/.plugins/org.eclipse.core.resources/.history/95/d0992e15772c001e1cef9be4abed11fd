package Fb_tc;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;

public class Login extends Driver
{
	
	 static By usrnam=By.xpath("//*[@id='email']");
	 static By passwd=By.xpath("//*[@type='password']");
	 static By login_xpath=By.xpath("//*[@type='submit']");
	 static By logo_xpath=By.xpath("//*[@class='fb_logo _8ilh img']");
	  
	 public static WebElement username()
	 {
	 WebElement username=d.findElement(usrnam);
	 return username;
	 }
	 public static WebElement password()
	 {
	 WebElement password=d.findElement(passwd);
	 return password;
	 }
	 public static WebElement login()
	 {
	 WebElement login=d.findElement(login_xpath);
	 return login;
	 }
	 public static void  username_enter(WebElement username)
	 {
		
		 username().click();
		 username().sendKeys("7993169534");
		 
		 
	 }
	 public static void  password_enter(WebElement username)
	 {
		 password().click();
		 password().sendKeys("Renu@ammu123");
	 }
	 public static void  login_click(WebElement username)
	 {
		 
		 login().click();
		 
	 }
	 public static WebElement logo(WebElement logo_xpath)
	 {
		WebElement logo=d.findElement(logo_xpath);
		return logo;
	 }
	
	
	
	
	
	
	

}
